  <system.windows.forms jitDebugging="true" />
  <appSettings>
    <!-- Default log level in the connect to OpenSpirit dialog.            -->
    <!-- Also applies to logging from some other parts of the system.      -->
    <!-- Valid choices are 0(off) to 3(verbose). For the Window logging,   -->
    <!-- 0 and 3 are discouraged. If 0, important information may be lost. -->
    <!-- 3 may cause performance issues because the log window is not      -->
    <!-- designed to handle large amounts of text. The log file is OK.     -->
    <add key="salmonLogToFile" value="0" />
    <add key="salmonLogToWindow" value="1" />
    <!-- Pop up an extra dialog about OpenSpirit context differences       -->
    <add key="openSpiritContextReport" value="Off" />
    <add key="normalizeSurveyOrigin" value="On" />
    <!-- Set this to true if updating of 2d OpenSpirit interpretation should  -->
    <!-- be done by deleting and re-creating the segments instead of update.  -->
    <!-- This will likely run slower, but was required for some OpenSpirit    -->
    <!-- back-ends. Reportedly fixed in OpenSpirit v3.0.1 and later.          -->
    <add key="forceCreateOpenSpirit2dSegments" value="false" />
    <!-- By default only the "Petrel CRS Catalog" is visible to the user.     -->
    <!-- Change this to "true" to allow all loaded catalogs in the service.   -->
    <add key="CRSDialogAllowCatalogSelection" value="true" />
    <!-- By default the user can select a NULL Coordinate Reference System.   -->
    <add key="CRSDialogAllowNullSelection" value="true" />
    <!-- By default Petrel will prime the coordinate service in a thread for faster startup.   -->
    <add key="PrimeCoordinateServiceAtStartup" value="true" />
    <!-- By default Petrel will prefill the archivable surrogate cache for faster loading/saving of projects -->
    <add key="PrefillArchivableSurrogateCache" value="true" />
    <!-- By default the tree will not expand in pick mode. Set the value to -->
    <!-- true to enable the automatic expansion                             -->
    <add key="ExpandTreeOnPicking" value="false" />
    <!-- By default the Clean Project History button is disabled.           -->
    <add key="DisableCleanProjectHistory" value="true" />
    <!-- By default the Multi SRD is enabled.                               -->
    <!-- Set value="true" (lower case) to disable.                          -->
    <add key="DisableMultiSRD" value="false" />
    <!-- Data SRD to Project SRD proposed time shift calculation method -->
    <!-- Set value="LowerSRD" to use the replacement velocity from the SRD with lower elevation (default).  -->
    <!-- Set value="UpperSRD" to use the replacement velocity from the SRD with higher elevation.  -->
    <!-- Set value="ProjectSRD" to use the replacement velocity from the project SRD.  -->
    <add key="ProposedTimeShiftCalculationMethod" value="LowerSRD" />
    <!-- Default method to calculate proposed shift for TDRs  -->
    <!-- Set value="From CS or OWT/TWT" to use the checkshot calculation as default  -->
    <!-- Set value="From SRD table" to use the data SRD to project SRD as default.  -->
    <add key="DefaultProposedShiftForTDRs" value="From CS or OWT/TWT" />
    <!-- List of initial modules for fast startup                           -->
    <add key="InitialModules" value="Core.ModuleM;Slb.Petrel.AppInfo.Module;Slb.Ocean.Petrel.Core.Impl.DomainObject.DomainObjectImplModule;Slb.Ocean.Petrel.PluginManagerGUI.Module;Slb.Petrel.DataInCloud.PetrelStorage.Integration.Module" />
    <!-- Enable fixes for high DPI problems in WinForms (eg. cursor scaling) -->
    <add key="EnableWindowsFormsHighDpiAutoResizing" value="true" />
    <!-- Default value for taper application method during wavelet extraction -->
    <!-- Set value="Center" to use the center mode as default in the wavelet extraction UI -->
    <!-- Set value="Edge" to use the edge mode as default in the wavelet extraction UI -->
    <add key="DefaultWaveletTaperApplicationMode" value="Edge" />
  </appSettings>
